{
  "extends": [
    "config:base",
    ":gitSignOff",
    ":disableDependencyDashboard",
    ":semanticCommitsDisabled"
  ],
  "postUpgradeTasks": {
    "commands": [
      "make gen-golden-all"
    ],
    "fileFilters": [ "tests/golden/**" ],
    "executionMode": "update"
  },
  "suppressNotifications": [ "artifactErrors" ],
  "labels": [
    "dependency"
  ]
}
